client
ca net/keys/ca.crt
cert net/keys/clients.crt
key net/keys/clients.key
dev tun
proto udp
remote <%= @server %> 1194
resolv-retry infinite
nobind
persist-key
persist-tun
mute-replay-warnings
ns-cert-type server
verb 3
mute 20
cipher none
topology subnet
ifconfig <%= @client_ip %> 255.255.255.0
#ifconfig-push <%= @client_ip %> 10.100.1.1